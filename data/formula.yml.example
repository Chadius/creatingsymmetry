sample_source_filename: example/rainbow_stripe.png
output_filename: ./out/example_image.png
output_size:
  width: 198
  height: 198
sample_space:
  minx: -8e-1
  miny: -8e-1
  maxx: 8e-1
  maxy: 8e-1
color_value_space:
  minx: -4e1
  maxx: 4e1
  miny: -6e1
  maxy: 2e1
rosette_formula:
  terms:
    - multiplier:
        real: -1.0
        imaginary: 2e-2
      power_n: 3
      power_m: 0
      coefficient_relationships:
        - -M-N
        - "+M+NF(N+M)"
